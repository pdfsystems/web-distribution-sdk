{
    "name": "pdfsystems/web-distribution-sdk",
    "description": "PHP SDK for Web Distribution",
    "keywords": [
        "pdf-systems",
        "web-distribution",
        "distribution",
        "sdk"
    ],
    "homepage": "https://github.com/pdfsystems/web-distribution-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Pungello",
            "email": "rob@pdfsystems.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "rpungello/sdk-client": "^1.11",
        "spatie/data-transfer-object": "^3.8",
        "symfony/validator": "^6.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Pdfsystems\\WebDistributionSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdfsystems\\WebDistributionSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
